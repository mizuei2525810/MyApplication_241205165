<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="明示的Intent"
        tools:layout_editor_absoluteX="76dp"
        tools:layout_editor_absoluteY="72dp"
        app:layout_constraintTop_toTopOf="@id/main"
        app:layout_constraintStart_toStartOf="@id/main"
        app:layout_constraintEnd_toStartOf="@id/button3"
        />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="暗黙的Intent"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="70dp"
        app:layout_constraintTop_toTopOf="@id/button2"
        app:layout_constraintStart_toEndOf="@id/button2"
        app:layout_constraintEnd_toEndOf="@id/main"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:text="これは表示されている"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="156dp"
        app:layout_constraintTop_toBottomOf="@id/button2"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="これは最初は表示されている"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="212dp"
        app:layout_constraintTop_toBottomOf="@id/textView"
        app:layout_constraintStart_toStartOf="@id/textView"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="これも最初は表示されている"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="265dp"
        app:layout_constraintTop_toBottomOf="@id/textView2"
        app:layout_constraintStart_toStartOf="@id/textView"/>

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="252dp"
        app:layout_constraintStart_toStartOf="@id/main"
        app:layout_constraintEnd_toStartOf="@id/button20"
        app:layout_constraintTop_toBottomOf="@id/textView3"/>

    <Button
        android:id="@+id/button20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="送信"
        tools:layout_editor_absoluteX="285dp"
        tools:layout_editor_absoluteY="246dp"
        app:layout_constraintStart_toEndOf="@id/editTextText2"
        app:layout_constraintTop_toTopOf="@id/editTextText2"
        app:layout_constraintEnd_toEndOf="@id/main"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Result:"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@id/button21"
        app:layout_constraintStart_toStartOf="@id/main"
        app:layout_constraintTop_toBottomOf="@id/editTextText2" />

    <Button
        android:id="@+id/button21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="起動"
        tools:layout_editor_absoluteX="285dp"
        tools:layout_editor_absoluteY="312dp"
        app:layout_constraintTop_toTopOf="@id/textView6"
        app:layout_constraintEnd_toEndOf="@id/main"
        app:layout_constraintStart_toEndOf="@id/textView6"/>
</androidx.constraintlayout.widget.ConstraintLayout>